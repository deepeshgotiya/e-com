 <%= form_with(model: [:admin, @product_category], local: true) do |form| %>
  <legend>Product Category Form</legend>

  <div class="mb-3">
    <%= form.label :name, class: "form-label" %>
    <%= form.text_field :name, id: "disabledTextInput", class: "form-control", placeholder: "Enter category name" %>
  </div>

  <div class="mb-3">
    <%= form.label :description, class: "form-label" %>
    <%= form.text_field :description, id: "disabledTextInput", class: "form-control", placeholder: "Enter Category Description" %>
  </div>

  <%= form.submit "Submit", class: "btn btn-primary" %>
<% end %>